{
  "name": "vite-mpa-template",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:test": "vite --mode test",
    "build": "cross-env VITE_APP_ENV=production tsx scripts/build.ts",
    "build:test": "cross-env VITE_APP_ENV=test tsx scripts/build.ts",
    "preview": "vite preview",
    "gen:page": "tsx scripts/gen-page.ts",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss}\"",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "pinia": "^3.0.3",
    "vue-i18n": "^11.0.0",
    "vant": "^4.9.15",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@types/node": "^20.17.9",
    "@types/prompts": "^2.4.9",
    "@types/fs-extra": "^11.0.4",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "ora": "^8.1.1",
    "fs-extra": "^11.2.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "glob": "^11.0.0",
    "postcss": "^8.4.49",
    "postcss-rtlcss": "^5.5.1",
    "prettier": "^3.4.2",
    "prompts": "^2.4.2",
    "sass": "^1.83.0",
    "terser": "^5.37.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^5.4.11",
    "vue-tsc": "^2.1.10"
  }
}
